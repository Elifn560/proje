@model dynamic

<div class="row">
    @if (Model.Count == 0)
    {
        <h3 style=" color: red">DERS SEÇİMİNİZ ONAYLANMAMIŞTIR! </h3>
    }
    <div class="col-12">
        <h3>Transcript</h3>
        <div class=" table-responsive table--no-card m-b-30">
            <table class=" table table-borderless table-striped tablle-earning">
                <thead>
                    <tr>
                        <th>Dersin Adı</th>
                        <th>Öğretmen Adı</th>
                        <th>Kredisi</th>
                        <th>Dersin Tipi</th>
                    </tr>
                </thead>
                <tbody>

                    @foreach (dynamic studentCourse in Model)
                    {
                        <tr>
                            <td>@studentCourse.CourseName</td>
                              <td>@studentCourse.TeacherName</td>
                                <td>@studentCourse.CourseCredit</td>
                                <td>@(studentCourse.CourseType == false? "Seçmeli": "Zorunlu")</td>

                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
       
